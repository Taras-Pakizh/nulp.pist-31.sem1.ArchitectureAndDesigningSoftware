<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<DialogPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.feo.app.task.Target14">
    <content>
        <AnchorPane prefHeight="500.0" prefWidth="360.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" text="Точки" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <AnchorPane prefHeight="400.0" prefWidth="500.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
                    <children>
                        <TableView fx:id="table" editable="true" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
                            <columns>
                                <TableColumn prefWidth="75.0" text="X" fx:id="xColumn" />
                                <TableColumn fx:id="yColumn" prefWidth="75.0" text="Y" />
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                        <Button layoutX="10.0" layoutY="386.0" mnemonicParsing="false" onAction="#addButtonClick" prefWidth="30.0" text="+" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" />
                        <Button layoutX="70.0" layoutY="385.0" mnemonicParsing="false" onAction="#deleteButtonClick" prefWidth="30.0" text="-" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="45.0" />
                        <Button layoutX="122.0" layoutY="385.0" mnemonicParsing="false" onAction="#upButtonClick" prefWidth="30.0" text="/\" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="45.0" />
                        <Button layoutX="174.0" layoutY="385.0" mnemonicParsing="false" onAction="#downButtonClick" prefWidth="30.0" text="\\/" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="10.0" />
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </content>
</DialogPane>
